<%- include('../partials/header') %>

<h2>Edit</h2>
<table class="edit-workout">
  <thead>
    <tr>
      <th>Date</th>
      <th>Exercise</th>
      <th>Sets</th>
      <th>Reps</th>
      <th>Weight</th>
      <th></th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <form action="/workouts/<%= workout._id %>?_method=PUT" method="POST">
        <%= console.log(datestring) %>
      <td><input aria-required="true" type="datetime-local" name="workoutDate" value="<%= datestring %>"></td>
      <td><input aria-required="true" type="text" name="content" value="<%= workout.content %>"></td>
      <td><input aria-required="true" type="number" name="reps" value="<%= workout.reps %>"></td>
      <td><input aria-required="true" type="number" name="sets" value="<%= workout.sets %>"></td>
      <td><input aria-required="true" type="number" name="weight" value="<%= workout.weight %>"></td>
      <td><button type="submit">Update</button> </td>
      </form>
    </tr>
  </tbody>
</table>

<%- include('../partials/footer') %>